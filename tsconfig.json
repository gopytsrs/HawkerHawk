{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"baseUrl": "./",
		"paths": {
			"@styles/*": ["styles/*"],
			"@components/*": ["components/*"],
			"@services/*": ["services/*"],
			"@utils/*": ["utils/*"],
			"@interfaces/*": ["interfaces/*"]
		},
		"incremental": true
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/HawkerCentres"],
	"exclude": ["node_modules"]
}
